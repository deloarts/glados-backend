{
    "cSpell.words": [
        "adminuser",
        "adminusers",
        "apscheduler",
        "autoflush",
        "bothify",
        "bruteforce",
        "cifs",
        "coloredlogs",
        "COMPUTERNAME",
        "dateutil",
        "ecommerce",
        "fastapi",
        "glados",
        "guestuser",
        "guestusers",
        "ilike",
        "jsonable",
        "levelname",
        "multilog",
        "mylowercasehost",
        "openpyxl",
        "partnumber",
        "passlib",
        "pydantic",
        "Pylance",
        "pylint",
        "pylintrc",
        "pyproject",
        "pytest",
        "qualname",
        "rcfile",
        "sessionmaker",
        "sqlalchemy",
        "starlette",
        "systemuser",
        "uvicorn"
